                        <h4> Za kwotÄ™:<%= @product.price%></h4>
                        <div class="contact-form-area">
                            <%= form_for @order do %>
                            <%= hidden_field_tag "product_id", params[:product_id]%>
                                <div class="form-group">
                                    <%= text_field_tag(:date_from, "", class: "form-control datetimepicker-input", placeholder: "Wpisz date od kiedy, format dd-mm-rrrr") %>
                                </div>
                                <%= submit_tag "Zamow", class: "btn fitness-btn btn-2 mt-30" %>
